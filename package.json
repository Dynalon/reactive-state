{
  "name": "reactive-state",
  "version": "3.6.1-0",
  "description": "Redux-like state management using RxJS and TypeScript",
  "main": "src/index.js",
  "files": [
    "src/**/*.js",
    "src/**/*.js.map",
    "src/**/*.d.ts",
    "react/**/*.js",
    "react/**/*.js.map",
    "react/**/*.d.ts"
  ],
  "types": "src/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "build-tests": "tsc -p test",
    "coverage": "node node_modules/.bin/istanbul cover _mocha -- test/test",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "prepublishOnly": "npm run build",
    "prettier": "prettier --write {src,test,react}/**/*.{ts,tsx}",
    "watch": "tsc -w --preserveWatchOutput",
    "watch-tests": "tsc -w -p test --preserveWatchOutput",
    "run-tests": "mocha --timeout 10000 test/test.js",
    "test": "npm run build-tests && npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dynalon/reactive-state.git"
  },
  "keywords": [
    "Redux",
    "State",
    "reactive",
    "RxJS",
    "store",
    "React"
  ],
  "author": "Timo DÃ¶rr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dynalon/reactive-state/issues"
  },
  "homepage": "https://github.com/Dynalon/reactive-state",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/enzyme": "^3.1.13",
    "@types/jsdom": "^12.2.3",
    "@types/lodash.isobject": "^3.0.3",
    "@types/lodash.isplainobject": "^4.0.3",
    "@types/mocha": "^5.2.6",
    "@types/node": "^13.1.1",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.2.0",
    "jsdom": "^15.2.1",
    "mocha": "^6.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "prettier": "^2.1.2"
  },
  "dependencies": {
    "lodash.isobject": "^3.0.2",
    "lodash.isplainobject": "^4.0.6",
    "rxjs": "^7.0.0-beta.7",
    "typescript": "^4.1.0-dev.20200926"
  },
  "optionalDependencies": {
    "istanbul": "^0.4.5",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "redux": "^4.0.0"
  }
}
